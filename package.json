{
  "name": "go-gitignore",
  "version": "1.0.0",
  "description": "Generate .gitignore files for your project",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nurlansu/go-gitignore.git"
  },
  "scripts": {
    "autoprefixer": "postcss -c package.json -r ./public/static/css/*.css -u autoprefixer",
    "sass": "wt compile ./public/source/sass/ -b ./public/static/css/ -s compressed",
    "sass:ci": "node-sass ./public/source/sass/ -o ./public/static/css/ --output-style compressed",
    "sass:watch": "wt watch ./public/source/sass/ -b ./public/static/css/ -s compressed",
    "build": "npm run sass && npm run autoprefixer",
    "build:ci": "npm run sass:ci && npm run autoprefixer"
  },
  "autoprefixer": {
    "browsers": "> 1%, last 2 versions, ie >= 9"
  },
  "dependencies": {

  },
  "devDependencies": {
    "autoprefixer": "^6.7.7",
    "node-sass": "^4.5.1",
    "postcss-cli": "^3.0.0"
  },
  "author": "Nurlan Su",
  "license": "MIT"
}
