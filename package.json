{
  "name": "go-gitignore",
  "version": "1.0.0",
  "description": "Generate .gitignore files for your project",
  "author": "Nurlan Su",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nurlansu/go-gitignore.git"
  },
  "scripts": {
    "autoprefixer": "postcss -c package.json -r ./public/static/css/*.css -u autoprefixer",
    "sass": "sass public/source/sass:public/static/css -s compressed",
    "sass:watch": "sass public/source/sass:public/static/css -s compressed --watch",
    "build": "npm run sass && npm run autoprefixer"
  },
  "dependencies": {},
  "devDependencies": {
    "autoprefixer": "^10.2.4",
    "postcss": "^8.2.6",
    "postcss-cli": "^8.3.1",
    "sass": "^1.32.7"
  },
  "autoprefixer": {
    "browsers": "> 1%, last 2 versions, ie >= 9"
  }
}
