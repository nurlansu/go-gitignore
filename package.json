{
  "name": "go-gitignore",
  "version": "1.0.0",
  "description": "Generate .gitignore files for your project",
  "author": "Nurlan Su",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nurlansu/go-gitignore.git"
  },
  "scripts": {
    "autoprefixer": "postcss -c package.json -r ./public/static/css/*.css -u autoprefixer",
    "sass": "node-sass ./public/source/sass/ -o ./public/static/css/ --output-style compressed",
    "sass:watch": "node-sass ./public/source/sass/ -wo ./public/static/css/ --output-style compressed",
    "build": "npm run sass && npm run autoprefixer"
  },
  "dependencies": {},
  "devDependencies": {
    "autoprefixer": "^9.6.1",
    "node-sass": "^4.12.0",
    "postcss-cli": "^6.1.3"
  },
  "autoprefixer": {
    "browsers": "> 1%, last 2 versions, ie >= 9"
  }
}
